      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "# Define\n",
        "status['label']=np.where(status['max_status']>=4, 1,0)\n",
        "# Validate\n",
        "status.groupby('label')['label'].count()"
      ],
